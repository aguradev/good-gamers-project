{
    "private": true,
    "scripts": {
        "homecss": "npx tailwindcss -i ./public/storage/tailwind-css/tailwind-home.css -o ./storage/app/public/css/style.css --watch",
        "admincss": "npx tailwindcss -i ./public/storage/tailwind-css/tailwind-admin.css -o ./storage/app/public/css/admin.css --watch",
        "logincss": "npx tailwindcss -i ./public/storage/tailwind-css/tailwind-login.css -o ./storage/app/public/css/login.css --watch",
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production"
    },
    "devDependencies": {
        "autoprefixer": "^10.4.7",
        "axios": "^0.21",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "postcss": "^8.4.14",
        "tailwindcss": "^3.1.5"
    },
    "dependencies": {
        "daisyui": "^2.19.0"
    }
}
